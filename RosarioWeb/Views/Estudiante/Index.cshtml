@model IEnumerable<RosarioWeb.Models.Estudiantes>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PrimerNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SegundoNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrimerApellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SegundoApellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sexo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nacionalidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Departamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Domicilio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdmateriaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdmatriculaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdnotaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdtutorNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PrimerNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SegundoNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrimerApellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SegundoApellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sexo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nacionalidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Departamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Domicilio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdmateriaNavigation.Idmateria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdmatriculaNavigation.Idmatricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdnotaNavigation.Idnota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdtutorNavigation.Idtutor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idestudiante">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idestudiante">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idestudiante">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
