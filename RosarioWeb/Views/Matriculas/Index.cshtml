@model IEnumerable<RosarioWeb.Models.Matricula>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreCompleto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Seccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modalidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AñoLectivo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Modalidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AñoLectivo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idmatricula">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idmatricula">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idmatricula">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
